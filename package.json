{
  "name": "cdk-basics-cloudwatch-events-lambda",
  "version": "0.1.0",
  "author": "@tettzlab",
  "license": "MIT",
  "bin": {
    "cdk-basics-cloudwatch-events-lambda": "bin/cdk-basics-cloudwatch-events-lambda.js"
  },
  "scripts": {
    "build": "npm run tsc && npm run test && npm run synth",
    "cdk": "cdk",
    "clean": "rimraf lib/*",
    "dist-clean": "rimraf cdk.out lib/*",
    "lint": "eslint --ext .ts --no-error-on-unmatched-pattern src tests --resolve-plugins-relative-to .",
    "lint-fix": "eslint --fix --ext .ts --fix --no-error-on-unmatched-pattern src tests --resolve-plugins-relative-to .",
    "rebuild": "npm run dist-clean && npm run build",
    "synth": "cdk synth",
    "test": "npm run lint && npm run jest",
    "jest": "jest",
    "tsc": "tsc --skipLibCheck",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.110",
    "@types/jest": "^29.4.0",
    "@types/node": "18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "aws-cdk": "^2.64.0",
    "esbuild": "^0.17.8",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.4.2",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.64.0",
    "aws-sdk": "^2.1315.0",
    "constructs": "^10.1.251",
    "source-map-support": "^0.5.21"
  }
}
